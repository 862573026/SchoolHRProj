@using sl.model
@using sl.extension

@model T_UserType
@{
    ViewBag.Title = "编辑用户类型";
    Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
}
<form class="validate" method="post" autocomplete="off" action="" enctype="multipart/form-data">
        <table class="msgtable" style="width:100%;height:100%">
            <tr>
                <th style="width: 130px">
                    账户类型名称：
                </th>
                <td>
                    @Html.InputFor(m => m.U_LoginTypeValue, Html.Valid().Required("请输入用户名！").Remote(Url.Action("CheckUserTypeIsExist"), "U_LoginTypeValue").AsHtmlAttribute())
            </tr>

        </table>
</form>
