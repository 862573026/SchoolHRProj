@using sl.model
@using sl.extension

@model T_FLType
    @{
        ViewBag.Title = "编辑友情链接类型";
        Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
    }
    <form class="validate" method="post" autocomplete="off" action="" enctype="multipart/form-data">
        <table class="msgtable" title="编辑下载类型" style="width:100%;height:100%">
            <tr>
                <th style="width: 130px">
                    友情链接类型名称：
                </th>
                <td>
                    @Html.InputFor(m => m.flTypeValue, Html.Valid().Required("请输入友情链接类型")
                .Remote(Url.Action("CheckDMTypeIsExist"), "dmTypeValue").AsHtmlAttribute())
            </tr>
        </table>
    </form>
