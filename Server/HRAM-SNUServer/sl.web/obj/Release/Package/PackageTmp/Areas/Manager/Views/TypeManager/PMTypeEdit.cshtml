@using sl.model
@using sl.extension

@model T_PMType
@{
    ViewBag.Title = "编辑发布类型";
    Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
}
<form class="validate" method="post" autocomplete="off" action="" enctype="multipart/form-data">
        <table class="msgtable" title="编辑发布类型" style="width:100%;height:100%">
            <tr>
                <th style="width: 130px">
                    发布类型名称：
                </th>
                <td>
                    @Html.InputFor(m => m.pmTypeValue, Html.Valid().Required("请输入发布类型！").Remote(Url.Action("CheckPMTypeIsExist"), "pmTypeValue").AsHtmlAttribute())
            </tr>

        </table>
</form>
